{{#each task}}
<div>
    <label>Task: </label>{{name}}<BR />
    <label>Status: </label>{{status}}<BR />
    <label>Deadline: </label>{{deadline}}<BR />
    <button type="button" class="collapsible">Description</button>
    <div class="content">
        <p>{{description}}</p>
        <p>{{responsible}}</p>
        <p>{{ETA}}</p>
    </div>
</div>
{{/each}}