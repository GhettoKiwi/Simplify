{{#each task}}
<button type="button" onclick = "getTask(this.data)" class="collapsible" data="{{id}}">{{name}} - {{status}}</button>
<div class="content">
    <label>Deadline: </label>
    <p>{{deadline}}</p>
    <label>Beskrivelse: </label>
    <p>{{description}}</p>
    <label>Ansvarlig: </label>
    <p>{{responsible}}</p>
    <label>ETA: </label>
    <p>{{ETA}}</p>
</div>
{{/each}}