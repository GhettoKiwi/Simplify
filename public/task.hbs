{{#each task}}
<div>
    <label>Deadline: </label>{{deadline}}<BR />
    <button type="button" class="collapsible">{{name}} - {{status}}</button>
    <div class="content">
        <p>{{deadline}}</p>
        <p>{{description}}</p>
        <p>{{responsible}}</p>
        <p>{{ETA}}</p>
    </div>
</div>
{{/each}}